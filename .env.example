# .env.example

# Supabase Configuration

# URL to your Supabase project (found in the Supabase dashboard)
# This is the base URL for your Supabase instance
# Example: https://pzmjkztloempaiyzhbou.supabase.co
NEXT_PUBLIC_SUPABASE_URL=https://example.supabase.co



# Anon/public key for client-side operations (found in API settings)
# This key is used for client-side operations and should be safe to expose
# Example: eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZSIsInJlZiI6InB6bWprenRsb2VtcGFpeXpoYm91Iiwicm9sZSI6ImFub24iLCJpYXQiOjE3NDM2NjE1MTMsImV4cCI6MjA1OTIzNzUxM30.1psHXiDE-0sDvrnXSachy5X8c8Pc3o9oN0jt8faIHpY
NEXT_PUBLIC_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.example-anon-key



# Service role key for secure server-side operations (keep this secret!)
# This key should be kept secret and used only on the server side
# Example: eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZSIsInJlZiI6InB6bWprenRsb2VtcGFpeXpoYm91Iiwicm9sZSI6InNlcnZpY2Vfcm9sZSIsImlhdCI6MTc0MzY2MTUxMywiZXhwIjoyMDU5MjM3NTEzfQ.MxT2qv_3196l0WDqTMn3RuNOEYf1eJlUy7SG48305xA
SUPABASE_SERVICE_ROLE_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.example-service-key



# Application Settings

# Base URL where your application is hosted (used for callbacks and absolute URLs)
# This is the URL where your application is accessible locally
# Example: http://localhost:3000
NEXT_PUBLIC_SITE_URL=http://localhost:3000



# Developer Settings

# Code used for developer quick login (development only)
#Example: 1234
NEXT_PUBLIC_DEVELOPER_CODE=1234

# gemeini Api key
# this will be used to access the gemeini api for the chat bot functionality
GEMINI_API_KEY=your_gemini_api_key_here 